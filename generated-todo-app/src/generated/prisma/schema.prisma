// Auto-generated by ShepLang
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        String   @id @default(uuid())
  title     String
  done      Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
