{
  "name": "@goldensheepai/sheplang-compiler",
  "version": "0.1.2",
  "description": "The AI-native compiler that transforms ShepLang into production-ready React, TypeScript, and API code",
  "keywords": [
    "sheplang",
    "compiler",
    "transpiler",
    "code-generation",
    "ai-native",
    "react",
    "typescript",
    "api",
    "handlebars",
    "full-stack"
  ],
  "author": "Golden Sheep AI",
  "license": "MIT",
  "homepage": "https://sheplang.lovable.app",
  "bugs": {
    "url": "https://github.com/Radix-Obsidian/Sheplang-BobaScript/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Radix-Obsidian/Sheplang-BobaScript.git",
    "directory": "sheplang/packages/compiler"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b",
    "test": "vitest --run",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "typescript": "^5.6.3",
    "@goldensheepai/sheplang-language": "workspace:*",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "vitest": "^1.6.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0"
  }
}
