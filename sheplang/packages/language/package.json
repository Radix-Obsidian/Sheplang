{
  "name": "@goldensheepai/sheplang-language",
  "version": "0.1.7",
  "description": "The first AI-native programming language parser with built-in verification",
  "keywords": [
    "sheplang",
    "ai-native",
    "programming-language",
    "parser",
    "langium",
    "type-safe",
    "full-stack",
    "compiler"
  ],
  "author": "Golden Sheep AI",
  "license": "MIT",
  "homepage": "https://sheplang.lovable.app",
  "bugs": {
    "url": "https://github.com/Radix-Obsidian/Sheplang-BobaScript/issues"
  },
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Radix-Obsidian/Sheplang-BobaScript.git",
    "directory": "sheplang/packages/language"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate": "langium generate",
    "prebuild": "langium generate",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run --dir .",
    "clean": "rimraf dist lib src/generated"
  },
  "dependencies": {
    "chevrotain-allstar": "^0.3.1",
    "langium": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "langium-cli": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  }
}
