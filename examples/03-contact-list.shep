app MyContacts

data Contact:
  fields:
    name: text
    email: text
    phone: text
    notes: text
  rules:
    - "name is required"
    - "email must be valid"

view ContactList:
  list Contact
  button "Add Contact" -> AddContact

action AddContact(name, email, phone, notes):
  add Contact with name, email, phone, notes
  show ContactList
